<main>
      <h1>Agregar Grupo</h1>
    <form action="/agregar-grupo" method="POST">
        <div>
            <label for="NombreGrupo">Nombre del Grupo:</label>
            <input type="text" id="NombreGrupo" name="NombreGrupo" required>
        </div>
        <div>
            <label for="SubCoordinadores">SubCoordinadores:</label>
            <input type="text" id="SubCoordinadores" name="SubCoordinadores" required>
        </div>
        <div>
            <label for="Coordinadoresgeneral">Coordinadores Generales:</label>
            <input type="text" id="Coordinadoresgeneral" name="Coordinadoresgeneral" required>
        </div>
        <div>
            <label for="Dirigentes">Dirigentes:</label>
            <input type="text" id="Dirigentes" name="Dirigentes" required>
        </div>
        <div>
            <label for="totalVotos">Total de Votos:</label>
            <input type="number" id="totalVotos" name="totalVotos" required>
        </div>
        <button type="submit">Agregar Grupo</button>
      
    </form>
</main>