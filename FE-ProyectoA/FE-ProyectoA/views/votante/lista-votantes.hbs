<main>
    <div class="row mt-3">
    <div class="row">
      <h1 class="text-center fw-bold " style="color: #1A2238;">Lista de votantes</h1>
    </div>
    <div class="table-responsive">
      <table class="table table-bordered table-striped">
        <thead>
          <tr>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Cedula</th>
             <th>Provincia</th>
              <th>Sector</th>
               <th>Telefono</th>
          </tr>
        </thead>
        <tbody>
          {{#each votantes}}
          <tr>
            <td>{{this.name}}</td>
            <td>{{this.description}}</td>
          </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</main>